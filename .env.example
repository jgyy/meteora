# ============ SOLANA ============
# Solana RPC endpoint
NEXT_PUBLIC_SOLANA_RPC_URL=https://api.devnet.solana.com

# Solana network (devnet, testnet, mainnet-beta)
NEXT_PUBLIC_SOLANA_NETWORK=devnet

# Cluster (devnet, testnet, mainnet)
NEXT_PUBLIC_CLUSTER=devnet

# ============ METEORA PROGRAM ============
# Meteora Tokenization Program ID (Solana public key)
NEXT_PUBLIC_METEORA_PROGRAM_ID=TokenizationProgram11111111111111111111111

# ============ WALLET ============
# Wallet provider endpoints (optional)
NEXT_PUBLIC_WALLET_PROVIDER=phantom

# ============ API ============
# Optional: Backend API endpoint if using one
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# ============ ANALYTICS ============
# Optional: Analytics/tracking keys
NEXT_PUBLIC_ANALYTICS_ID=

# ============ DEVELOPMENT ============
# Debug mode
DEBUG=false

# Log level (error, warn, info, debug)
LOG_LEVEL=info
